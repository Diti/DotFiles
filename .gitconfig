[alias]
	co = checkout
	cm = commit
	slog = log --show-signature
	smerge = merge -S --no-ff
	st = status
[color]
	ui = true
[commit]
	gpgSign = true
[core]
	autocrlf = input
	editor = vim
	excludesFile = ~/.gitignore_global
[gpg]
	program = gpg2
[help]
	autoCorrect = 1
[push]
	default = simple
[user]
	signingkey = 0xFD4F1D56645219A0C6F6F9AB31A49121CD42FF00
	email = kra@diti.me
	name = Diti Torterat
